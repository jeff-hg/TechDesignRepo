#include <PRIZM.h>
PRIZM prizm;

int motor_one_power = 25
int motor_two_power = 25
int LeftMotor = 1
int RightMotor = 2

void setup() {        

  prizm.PrizmBegin();
}

void loop(int motor_one_power, int motor_two_power) {

  prizm.setMotorPower(LeftMotor,motor_one_power);
  prizm.setMotorPower(RightMotor,motor_two_power);
  
}
